type AppliedPromotionResponse {
  promotion: AppliedPromotionDetailResponse!
  involvedCartItems: [Int!]!
}

type AppliedPromotionDetailResponse {
  id: String!
  description: String!
  code: String
  type: PromotionType!
  freeDetail: FreePromotionDetailResponse
}

type FreePromotionDetailResponse {
  ean: String!
  units: Int!
  quantity: Int!
  image: String!
}

enum PromotionType {
  DISCOUNT, FREE
}